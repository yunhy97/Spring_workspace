<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="kr.co.jhta.dao.RoleDao">
	<select id="getRolesUserById" resultType="string" parameterType="string" >
		select has_role
		from spring_user_roles
		where user_id = #{value} 
	</select>
	<select id="getRolesByUrl" resultType="string" parameterType="string">
		select req_role
		from spring_url_roles
		where req_url = #{value}
	</select>
</mapper>